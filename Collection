package target;

public class collection {
	private long firstRadius;
	private long amtOfPaint;
	
	public collection(long inputRadius, long inputAmtOfPaint) {
		firstRadius = inputRadius;
		amtOfPaint = inputAmtOfPaint;
	}
	
	public long getFirstRadius() {
		return firstRadius;
	}
	
	public long getAmtOfPaint() {
		return amtOfPaint;
	}
}
